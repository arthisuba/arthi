#include<stdio.h>
int main()
{
char r;
printf("enter the single charcter:\n");
scanf("%c",&r);
if((r='a')||(r='e')||(r='i')||(r='o')||(r='u')||(r='A')||(r='E')||(r='I')||(r='O')||(r='U'))
{
printf("the given character %c is vowel\n",r);
}
else
{
printf("the given character %c is consonant\n",r);
}
return 0;
}
